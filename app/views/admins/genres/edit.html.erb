<h1> 分野名の編集 </h1><br>

<div class="navbar navbar-default">
  <div class= "navbar-inner">
    <ul class="nav">
      <li><%= link_to "分野一覧へ", admins_genres_path %></li>
    </ul>
  </div>
</div>

<div class="editer_container">
  <%= form_for @genre, url: admins_genre_path do |f| %>
    <%= f.label :分野名 %>
    <%= f.text_field :genre_name %>
    <%= f.submit :分野名の編集 %>
  <% end %>
</div>
